@import(prelude)


data Maybe[$1] = just($1 content) | none();

fn map(x_fn : fn($1) -> $2) -> fn(list[$1]) -> list[$2]{
    return |list[$1] x_list| -> list[$2] {
        return [x_fn(x) | $1 x <- x_list];
    }
}

fn add1(int x) -> int {
    return x + 1;
}

fn main(args : list[str]) -> void {
	print("hello world\n");
    var result = map(add1)([1,2,3,4]);
    print(result);
}


